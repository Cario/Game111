 Create table pokemans(
  id int auto_increment primary key,
  name varchar(30),
  type varchar(30),
  gender char(1),
  height decimal(5,1)
  );
 // (Pokichu, Squortle, Bolbasore, Chalizard) 

// Command "drop table" removes a table, and "if exists" does as it implies
drop table if exists customers;
drop table if exists Orders;
drop table if exists customers;
drop table if exists Shippings;

//The command to insert data into the table
insert into pokemans (id, name, gender, type, height)
values 
(4, "Bolbasore", "M", "Grass", 90.5),
(5, "Unknown", "N", "Normal", 1.1)
;
//To remove certain rows with no Id number
delete from Pokemans
where id is null;
